# Database host (usually 'db' when using Docker Compose)
DB_HOST=db
# Database username - change this to your preferred database username
DB_USER=your_db_user
# Database password - change this to your preferred database password
DB_PASSWORD=your_db_password
# Database name - change this to your preferred database name
DB_NAME=your_db_name
# Discord bot token (from Discord Developer Portal) https://discord.com/developers/applications/
DISCORD_BOT_TOKEN=your_bot_token